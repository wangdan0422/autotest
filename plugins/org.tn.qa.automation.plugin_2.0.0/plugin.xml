<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension-point id="splashExtension" name="Splash Extensions" schema="schema/splashExtension.exsd"/>

   <extension
         point="org.eclipse.ui.editors">
      <editor
            name="Map Editor"
            extensions="map"
            icon="icons/map.gif"
            contributorClass="org.tn.qa.automation.plugin.editors.MapEditorContributor"
            class="org.tn.qa.automation.plugin.editors.MapEditor"
            id="org.tn.qa.automation.plugin.editors.MapEditor">
      </editor>
      <editor
            class="org.tn.qa.automation.plugin.editors.testcase.TestCaseEditor"
            extensions="tc"
            icon="icons/tc.gif"
            id="org.tn.qa.automation.plugin.testcase"
            name="TestCase Editor">
      </editor>
      <editor
            class="org.tn.qa.automation.plugin.editors.testcase.TestSuiteEditor"
            extensions="ts"
            icon="icons/ts.gif"
            id="org.tn.qa.automation.plugin.testsuite"
            name="TestSuite Editor">
      </editor>
      <editor
            class="org.tn.qa.automation.plugin.editors.DataConfigEditor"
            default="true"
            extensions="properties"
            icon="icons/config_file.gif"
            id="org.tn.qa.automation.plugin.DataConfigEditor"
            name="DataConfig Editor">
      </editor>
      <editor
            class="org.tn.qa.automation.plugin.editors.TestPlanEditor"
            default="true"
            extensions="tp"
            icon="icons/testplan.png"
            id="org.tn.qa.automation.plugin.testplan"
            name="TAP TestPlan">
      </editor>
      <editor
            class="org.tn.qa.automation.plugin.editors.testcase.TestVerbsEditor"
            default="false"
            extensions="ks"
            icon="icons/keywordSet.png"
            id="org.tn.qa.automation.plugin.editors.keywordsetEditor"
            name="UserDefine Keyword Edtior">
      </editor>
      <editor
            class="org.tn.qa.automation.plugin.editors.RemoteTestPlanEditor"
            default="true"
            extensions="tpR"
            icon="icons/testplan.png"
            id="org.tn.qa.automation.plugin.remotetestplan"
            name="TAP RemoteTestPlan">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.newWizards">
      <category
            name="Automation Wizards"
            id="org.tn.qa.automation.plugin">
      </category>
      <wizard
            name="Map File"
            icon="icons/newmap.gif"
            category="org.tn.qa.automation.plugin"
            class="org.tn.qa.automation.plugin.wizards.MapNewWizard"
            id="org.tn.qa.automation.plugin.wizards.newmap">
      </wizard>
      <wizard
            category="org.tn.qa.automation.plugin"
            class="org.tn.qa.automation.plugin.wizards.PrjNewWizard"
            icon="icons/newprj.gif"
            id="org.tn.qa.automation.plugin.wizard.newprj"
            name="Automation Project"
            project="true">
      </wizard>
      <wizard
            category="org.tn.qa.automation.plugin"
            class="org.tn.qa.automation.plugin.wizards.TestCaseNewWizard"
            icon="icons/tc_new.gif"
            id="org.tn.qa.automation.plugin.wizards.newtestcase"
            name="TestCase File">
      </wizard>
   </extension>
   <extension
         id="org.tn.qa.automation.plugin.builder.autonature"
         name="Automation Project Nature"
         point="org.eclipse.core.resources.natures">
      <runtime>
         <run
               class="org.tn.qa.automation.plugin.builder.AutomationProjectNature">
         </run>
      </runtime>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="org.tn.qa.automation.plugin.perspectives.AutomationPerspective"
            icon="icons/releng_gears.gif"
            id="org.tn.qa.automation.plugin.perspectives.AutomationPerspective"
            name="Automation">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.jdt.core.classpathVariableInitializer">
      <classpathVariableInitializer
            class="org.tn.qa.automation.plugin.path.AutomationClasspathVariableInitializer"
            variable="AUTOMATION_LIB">
      </classpathVariableInitializer>
      <classpathVariableInitializer
            class="org.tn.qa.automation.plugin.path.SeleniumClasspathVariableInitializer"
            variable="SELENIUM_LIB">
      </classpathVariableInitializer>
      <classpathVariableInitializer
            class="org.tn.qa.automation.plugin.path.KeywordClasspathVariableInitializer"
            variable="KEYWORD_LIB">
      </classpathVariableInitializer>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <category
            id="org.tn.qa.automation.plugin"
            name="Automation Category">
      </category>
      <view
            category="org.tn.qa.automation.plugin"
            class="org.tn.qa.automation.plugin.views.TestCaseView"
            icon="icons/ts_view.gif"
            id="org.tn.qa.automation.plugin.views.TestCaseView"
            name="TestCase Explorer">
      </view>
      <view
            category="org.tn.qa.automation.plugin"
            class="org.tn.qa.automation.plugin.views.KeywordView"
            icon="icons/kw_view.gif"
            id="org.tn.qa.automation.plugin.views.KeywordView"
            name="Keyword">
      </view>
      <view
            category="org.tn.qa.automation.plugin"
            class="org.tn.qa.automation.plugin.views.MapView"
            icon="icons/map_view.gif"
            id="org.tn.qa.automation.plugin.views.MapView"
            name="Map Explorer">
      </view>
      <view
            category="org.tn.qa.automation.plugin"
            class="org.tn.qa.automation.plugin.views.DataConfigView"
            icon="icons/config.gif"
            id="org.tn.qa.automation.plugin.views.DataConfigView"
            name="DataConfig"
            related="org.tn.qa.automation.plugin.views.MapView"
			>
      </view>
      <view
            category="org.tn.qa.automation.plugin"
            class="org.tn.qa.automation.plugin.views.LogView"
            icon="icons/log_view.gif"
            id="org.tn.qa.automation.plugin.views.LogView"
            name="Log">
      </view>
       <view
            category="org.tn.qa.automation.plugin"
            class="org.tn.qa.automation.plugin.views.SearchView"
            icon="icons/find.png"
            id="org.tn.qa.automation.plugin.views.SearchView"
            name="Search">
      </view>
      <view
            category="org.tn.qa.automation.plugin"
            class="org.tn.qa.automation.plugin.views.UserDefinedKeywordView"
            icon="icons/keywordSet.png"
            id="org.tn.qa.automation.plugin.views.UserDefinedKeywordView"
            name="UserDefine"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.importWizards">
      <wizard
            class="org.tn.qa.automation.plugin.wizards.PrjImportWizard"
            icon="icons/openprj.gif"
            id="org.tn.qa.automation.plugin.wizard.openprj"
            name="Automation Project">
      </wizard>
   </extension>
