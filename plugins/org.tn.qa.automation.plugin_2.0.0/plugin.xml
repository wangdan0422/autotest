   <extension point="org.eclipse.help.toc">
      <toc
            file="toc.xml"
            primary="true">
      </toc>
	</extension>
   <extension
         id="satproduct"
         point="org.eclipse.core.runtime.products">
      <product
            application="org.tn.qa.automation.sat.application"
            description="Automation Tool"
            name="TAP">
         <property
               name="windowImages"
               value="icons/logo/牛宝16.gif,icons/logo/牛宝32.gif,icons/logo/牛宝48.gif,icons/logo/牛宝64.gif,icons/logo/牛宝128.gif">
         </property>
         <property
               name="startupForegroundColor"
               value="FF8000">
         </property>
         <property
               name="startupProgressRect"
               value="0,280,460,15">
         </property>
         <property
               name="startupMessageRect"
               value="15,250,441,20">
         </property>
         <property
               name="appName"
               value="TAP">
         </property>
         <property
               name="preferenceCustomization"
               value="plugin_customization.ini">
         </property>
         <property
               name="applicationCSS"
               value="platform:/plugin/org.tn.qa.automation.plugin/default.css">
         </property>
         <property
               name="applicationXMI"
               value="org.eclipse.platform/LegacyIDE.e4xmi">
         </property>
         <property
               name="cssTheme"
               value="org.eclipse.e4.ui.css.theme.e4_default">
         </property>
         <property
               name="applicationCSSResources"
               value="platform:/plugin/org.eclipse.platform/images/">
         </property>
      </product>
   </extension>

	<extension
       id="org.tn.qa.automation.sat.application"
       name="TAP"
       point="org.eclipse.core.runtime.applications">
	   <application
          icon="icons/logo/牛宝.ico"
          visible="true">
	      <run class="org.tn.qa.automation.plugin.application.TAPApplication"/>
	   </application>
	</extension>
 <extension
       point="org.eclipse.ui.splashHandlers">
    <splashHandler
          class="org.tn.qa.automation.plugin.splashHandlers.ExtensibleSplashHandler"
          id="org.tn.qa.automation.plugin.splashHandlers.extensible">
    </splashHandler>
    <splashHandler
          class="org.tn.qa.automation.plugin.splashHandlers.BrowserSplashHandler"
          id="org.tn.qa.automation.plugin.splashHandlers.browser">
    </splashHandler>
    <splashHandler
          class="org.tn.qa.automation.plugin.splashHandlers.InteractiveSplashHandler"
          id="org.tn.qa.automation.plugin.splashHandlers.interactive">
    </splashHandler>
 </extension>
 <extension
       point="org.tn.qa.automation.plugin.splashExtension">
    <splashExtension
          icon="icons/af.png"
          id="af"
          tooltip="Application Framework">
    </splashExtension>
    <splashExtension
          icon="icons/embedded.png"
          id="embedded"
          tooltip="Embedded">
    </splashExtension>
    <splashExtension
          icon="icons/enterprise.png"
          id="enterprise"
          tooltip="Enterprise">
    </splashExtension>
    <splashExtension
          icon="icons/languages.png"
          id="languages"
          tooltip="Languages">
    </splashExtension>
    <splashExtension
          icon="icons/rcp.png"
          id="rcp"
          tooltip="RCP">
    </splashExtension>
    <property name="preferenceCustomization" value="plugin_customization.ini">   
</property>
 </extension>

 <extension
       id="main"
       name="SATMenu"
       point="org.eclipse.ui.menus">
	<menuContribution
	allPopups="false"
	locationURI="toolbar:main?before=additions">
	</menuContribution>
</extension>
 <extension  
             point="org.eclipse.ui.activities">  
          <activity  
                id="activity1"  
                name="remove editor buttons">  
             <enabledWhen>  
                <with  
                      variable="activePartId">  
                   <equals  
                         value="activity1">  
                   </equals>  
                </with>  
             </enabledWhen>  
          </activity>  
          <activityPatternBinding  
                activityId="activity1"  
                isEqualityPattern="false"  
                pattern="org.eclipse.ui.edit.*">  
          </activityPatternBinding>
</extension>
 <extension
       point="org.eclipse.ui.popupMenus">
    <objectContribution
          adaptable="true"
          id="org.tn.qa.automation.plugin.team.menu"
          objectClass="org.eclipse.core.resources.IResource">
       <action
             class="org.eclipse.team.internal.ui.actions.ApplyPatchAction"
             id="org.tn.qa.automation.plugin.popup.action"
             label="Team"
             menubarPath="additions">
       </action>
    </objectContribution>
 </extension>
</plugin>
